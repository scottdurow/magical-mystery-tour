  <content>{"source":" {% fetchxml resultVariable %}\r\n&lt;fetch version=\"1.0\" mapping=\"logical\" no-lock=\"false\" distinct=\"true\"&gt;\r\n&lt;entity name=\"contoso_listing\"&gt;\r\n    &lt;attribute name=\"contoso_listingid\" /&gt;\r\n    &lt;attribute name=\"contoso_image_url\" /&gt;\r\n    &lt;attribute name=\"contoso_name\" /&gt;\r\n    &lt;attribute name=\"contoso_address\" /&gt;\r\n    &lt;attribute name=\"contoso_pricepermonth\" /&gt;\r\n    &lt;attribute name=\"contoso_features\" /&gt;\r\n    &lt;attribute name=\"contoso_displayname\" /&gt;\r\n    &lt;attribute name=\"contoso_description\" /&gt;\r\n    &lt;attribute name=\"contoso_primaryimage\" /&gt;\r\n    &lt;filter type=\"and\"&gt;\r\n    &lt;condition attribute=\"statecode\" operator=\"eq\" value=\"0\" /&gt;\r\n    {% if params.q %}\r\n    &lt;condition attribute=\"contoso_description\" operator=\"like\" value=\"%{{params.q}}%\" /&gt;\r\n    {% endif %}\r\n    &lt;/filter&gt;\r\n    &lt;order attribute=\"contoso_name\" descending=\"false\" /&gt;\r\n&lt;/entity&gt;\r\n&lt;/fetch&gt;\r\n{% endfetchxml %}\r\n\r\n\r\n&lt;div class=\"container-fluid\"&gt;\r\n    &lt;div class=\"justify-content-center row\"&gt;\r\n        &lt;div class=\"mb-3 mx-auto col-xl-4 col-lg-6 col-md-7 col-10\"&gt;\r\n            &lt;div class=\"flex-nowrap input-group\"&gt;\r\n            &lt;span class=\"bg-light text-light-primary input-group-text\"&gt;\r\n                &lt;svg stroke=\"currentColor\" fill=\"currentColor\" stroke-width=\"0\" viewBox=\"0 0 24 24\" height=\"2rem\" width=\"2rem\" xmlns=\"http://www.w3.org/2000/svg\"&gt;\r\n                &lt;path d=\"M10 18a7.952 7.952 0 0 0 4.897-1.688l4.396 4.396 1.414-1.414-4.396-4.396A7.952 7.952 0 0 0 18 10c0-4.411-3.589-8-8-8s-8 3.589-8 8 3.589 8 8 8zm0-14c3.309 0 6 2.691 6 6s-2.691 6-6 6-6-2.691-6-6 2.691-6 6-6z\"&gt;&lt;/path&gt;&lt;/svg&gt;&lt;/span\r\n            &gt;&lt;input placeholder=\"Search\" style=\"line-height:2\" class=\"bg-light text-black form-control\" value=\"{{params.q}}\" onkeydown='if (event.keyCode == 13) { window.location.href=\"?q=\"+encodeURIComponent(value.replace(/[&lt;&gt;]/g, \"\")); return false; }' /&gt;\r\n        &lt;/div&gt;\r\n    &lt;/div&gt;\r\n&lt;/div&gt;\r\n\r\n&lt;div class=\"bg-light-2\" style=\"height: 100vh; width: 100%; overflow-y: auto;\"&gt;\r\n    &lt;div class=\"py-4 container-fluid\"&gt;\r\n        &lt;div class=\"d-flex gap-3 justify-content-around row\"&gt;\r\n        {% for entity in resultVariable.results.entities %}\r\n        &lt;div class=\"bg-light text-black text-center p-0 overflow-hidden shadow mx-auto mb-4 card\" style=\"width: 18rem; height: auto;\"&gt;\r\n            &lt;a href=\"/Listing-Details-PCF/?id={{ entity.contoso_listingid }}\"&gt;\r\n            {% if entity.contoso_primaryimage %}\r\n            &lt;img lazyload=\"true\" class=\"card-img-top img-fluid w-100\" src=\"/Image/download.aspx?entity=contoso_listingimage&amp;amp;attribute=contoso_image&amp;amp;ID={{ entity.contoso_primaryimage.id}}&amp;amp;Full=true\" style=\"height: 15rem; object-fit: cover; object-position: center center;\" /&gt;\r\n            {% endif %}\r\n            &lt;/a&gt;\r\n            &lt;div class=\"d-flex flex-column card-body\"&gt;\r\n            &lt;div class=\"card-title h5\" style=\"text-overflow: ellipsis; overflow: hidden; white-space: nowrap;\"&gt;{{ entity.contoso_name }}&lt;/div&gt;\r\n            &lt;p class=\"card-text\"&gt;{{ entity.contoso_description }}&lt;/p&gt;\r\n            &lt;div class=\"d-flex justify-content-between mt-auto\"&gt;&lt;span&gt;${{ entity.contoso_pricepermonth | round: 2}}/month&lt;/span&gt;&lt;a href=\"/Listing-Details-PCF/?id={{entity.contoso_listingid}}\" class=\"bg-light-primary d-flex align-item-center ml-auto border-0 btn btn-primary\"&gt;View listing&lt;/a&gt;&lt;/div&gt;\r\n            &lt;/div&gt;\r\n        &lt;/div&gt;\r\n        {% endfor %}\r\n        &lt;/div&gt;\r\n    &lt;/div&gt;\r\n&lt;/div&gt;"}</content>
